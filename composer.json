{
    "name": "pecserke/yaml-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PecserkeYamlFixturesBundle",
    "keywords": [ "Data fixtures", "Doctrine", "YaML" ],
    "homepage": "https://github.com/tomas-pecserke/YamlFixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Pecserke",
            "email": "tomas.pecserke@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pecserke\\YamlFixturesBundle\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pecserke\\YamlFixturesBundle\\": ["test/"]
        }
    },
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "doctrine/data-fixtures": "^1.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/property-access": "^4.3",
        "symfony/finder": "^4.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "doctrine/mongodb-odm": "^2.0",
        "phpunit/phpunit": "^8.4",
        "php-coveralls/php-coveralls": "^2.1",
        "mikey179/vfsstream": "^1.6"
    },
    "suggest": {
        "doctrine/orm": "Support for relation database fixtures",
        "doctrine/mongodb-odm": "Support for mongo database fixtures",
        "doctrine/doctrine-migrations-bundle": "Database migrations help you version the changes in your database schema and apply them in a predictable way on every server running the application."
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
