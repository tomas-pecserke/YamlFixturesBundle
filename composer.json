{
    "name": "pecserke/yaml-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PecserkeYamlFixturesBundle",
    "keywords": [ "Data fixtures", "Doctrine" ],
    "homepage": "https://github.com/tomas-pecserke/YamlFixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomáš Pecsérke",
            "email": "tomas.pecserke@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Pecserke\\YamlFixturesBundle\\": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/data-fixtures": "~1.0",
        "symfony/framework-bundle": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/property-access": "~2.3"
    },
    "require-dev": {
        "doctrine/orm": "*",
        "doctrine/doctrine-bundle": "~1.3",
        "doctrine/mongodb-odm": "*@beta",
        "doctrine/mongodb-odm-bundle": "~3.0@beta",
        "jackalope/jackalope-doctrine-dbal": "*",
        "doctrine/phpcr-odm": "~1.2",
        "doctrine/phpcr-bundle": "~1.2",
        "phpunit/phpunit": "~4.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "Support for relational database fixtures",
        "doctrine/mongodb-odm-bundle": "Support for MongoDB database fixtures",
        "doctrine/phpcr-odm": "Support for PHPCR database fixtures"
    }
}
