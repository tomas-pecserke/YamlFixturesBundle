{
    "name": "pecserke/yaml-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PecserkeYamlFixturesBundle",
    "keywords": ["Data fixtures"],
    "homepage": "https://github.com/Pecserke/YamlFixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomáš Pecsérke",
            "email": "tomas@pecserke.eu",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": { "Pecserke\\YamlFixturesBundle": "" }
    },
    "target-dir": "Pecserke/YamlFixturesBundle",
    "require": {
        "php": ">=5.3.2",
        "symfony/http-kernel": ">=2.1,<2.2",
        "symfony/dependency-injection": ">=2.1,<2.2",
        "symfony/config": ">=2.1,<2.2",
        "symfony/console": ">=2.1,<2.2",
        "symfony/filesystem": ">=2.1,<2.2",
        "symfony/yaml": ">=2.1,<2.2",
        "doctrine/data-fixtures": "*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "doctrine/mongodb-odm": "*"
    },
    "minimum-stability": "dev"
}
