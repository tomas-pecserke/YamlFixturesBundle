{
    "name": "pecserke/yaml-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PecserkeYamlFixturesBundle",
    "keywords": [ "Data fixtures", "Doctrine" ],
    "homepage": "https://github.com/tomas-pecserke/YamlFixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomáš Pecsérke",
            "email": "tomas@pecserke.eu",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": { "Pecserke\\YamlFixturesBundle": "" }
    },
    "target-dir": "Pecserke/YamlFixturesBundle",
    "require": {
        "php": ">=5.3.3",
        "doctrine/data-fixtures": "1.*@alpha",
        "symfony/framework-bundle": ">=2.3,<3.0",
        "symfony/http-kernel": ">=2.3,<3.0",
        "symfony/dependency-injection": ">=2.3,<3.0",
        "symfony/config": ">=2.3,<3.0",
        "symfony/console": ">=2.3,<3.0",
        "symfony/filesystem": ">=2.3,<3.0",
        "symfony/yaml": ">=2.3,<3.0",
        "symfony/property-access": ">=2.3,<3.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": ">=2.3, <3.0",
        "doctrine/mongodb-odm": "1.0.*",
        "phpunit/phpunit": "~4.3"
    },
    "suggest": {
        "doctrine/orm": "Support for relation database fixtures",
        "doctrine/mongodb-odm": "Support for mongo database fixtures"
    },
    "minimum-stability": "beta"
}
